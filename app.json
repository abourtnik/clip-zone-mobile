{
  "expo": {
    "name": "ClipZone",
    "description": "Open Source React Native Youtube Clone",
    "slug": "clip-zone",
    "privacy": "public",
    "sdkVersion": "51.0.0",
    "version": "0.1.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/abourtnik/clip-zone-mobile",
    "orientation": "portrait",
    "backgroundColor": "#ffffff",
    "primaryColor": "#3D556B",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#3D556B"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D556B"
    },
    "ios": {
      "supportsTablet": false,
      "backgroundColor": "#3D556B"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D556B"
      },
      "package": "com.abourtnik.clipzone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "aadc51ac-7aa0-4198-9d25-25ff7fab2e1e"
      }
    }
  }
}
